{
  "name": "cakebackuppro/cake-backup-pro",
  "description": "CakePHP backup and restore plugin for Backblaze B2 with one-click full restore and component-level control.",
  "type": "cakephp-plugin",
  "license": "MIT",
  "keywords": [
    "cakephp",
    "backup",
    "restore",
    "backblaze",
    "b2"
  ],
  "require": {
    "php": ">=8.1",
    "cakephp/cakephp": "^5.0",
    "cakephp/migrations": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "CakeBackupPro\\": "src/"
    }
  },
  "extra": {
    "installer-name": "CakeBackupPro"
  }
}

